<div class="col-4 p-3">
  <div class="card">
    <img src="<%= dish.image %>" class="card-img-top">
    <div class="card-body">
      <h5 class="card-title"><%= link_to dish.name , dish , class: "text-decoration-none text-reset" %></h5>
      <p class="card-text"><%= dish.description %></p>
      <button type="button" class="btn btn-outline-primary" disabled><%= dish.category.name %></button>
    </div>
  </div>
</div>
